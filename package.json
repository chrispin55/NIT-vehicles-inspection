{
  "name": "nit-vehicle-management",
  "version": "1.0.8",
  "description": "Integrated Transport & Vehicle Management System for NIT University Dar es Salaam",
  "keywords": [
    "vehicle",
    "management",
    "transport",
    "university",
    "nit"
  ],
  "license": "MIT",
  "author": "NIT University Dar es Salaam",
  "type": "commonjs",
  "main": "server.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest",
